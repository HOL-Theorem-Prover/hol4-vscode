{
  "name": "hol4-mode",
	"displayName": "HOL4 mode",
	"description": "A VSCode extension for interacting with the HOL4 theorem prover",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:hol4-mode.startSession",
		"onCommand:hol4-mode.stopSession",
		"onCommand:hol4-mode.sendSelection",
		"onCommand:hol4-mode.sendUntilCursor",
		"onCommand:hol4-mode.sendGoal",
		"onCommand:hol4-mode.sendTactic"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "hol4-mode.startSession",
				"title": "HOL: Start session"
			},
			{
				"command": "hol4-mode.stopSession",
				"title": "HOL: Stop session"
			},
			{
				"command": "hol4-mode.sendSelection",
				"title": "HOL: Send selection"
			},
			{
				"command": "hol4-mode.sendUntilCursor",
				"title": "HOL: Send all up to current line"
			},
			{
				"command": "hol4-mode.sendGoal",
				"title": "HOL: Send goal"
			},
			{
				"command": "hol4-mode.sendTactic",
				"title": "HOL: Send tactic"
			}
		],
		"keybindings": [
			{
				"command": "hol4-mode.startSession",
				"key": "ctrl+k h h",
				"mac": "cmd+k h h",
				"when": "editorLangId == sml"
			},
			{
				"command": "hol4-mode.stopSession",
				"key": "ctrl+k h x",
				"mac": "cmd+k h x",
				"when": "editorLangId == sml"
			},
			{
				"command": "hol4-mode.sendSelection",
				"key": "ctrl+k h s",
				"mac": "cmd+k h s",
				"when": "editorLangId == sml"
			},
			{
				"command": "hol4-mode.sendUntilCursor",
				"key": "ctrl+k h enter",
				"mac": "cmd+k h enter",
				"when": "editorLangId == sml"
			},
			{
				"command": "hol4-mode.sendGoal",
				"key": "ctrl+k h g",
				"mac": "cmd+k h g",
				"when": "editorLangId == sml"
			},
			{
				"command": "hol4-mode.sendTactic",
				"key": "ctrl+k h e",
				"mac": "cmd+k h e",
				"when": "editorLangId == sml"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"@vscode/test-electron": "^2.0.3"
	}
}
